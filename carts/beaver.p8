version 42
__lua__
function _init()
 p={
 x=63,
 y=63
 }
 fx=false
 fy=false
 sp=1
 timer=0
 sx=speed
 sy=3
 speed=2
 spetimer=5
end

function _update()
 if btn(➡️) then
  p.x+=2
  fx=false
  fy=false
  if(sp == 13) sp=1
 end
 if btn(⬅️)then 
  p.x-=2
  fx=true
  fy=false
  if(sp == 13) sp=1
 end
 if btn(⬇️) then
  p.y+=2
  fy=true
  sp=13
 end
 if btn(⬆️) then
  p.y-=sy 
  sp=13
  fy=false
 end 
 if sp<13 then 
	 if timer<5 then
	  timer+=1
	 else
		 if sp<11 then
		  sp+=2
		 else
		  sp=1
		 end
		 timer=0
		end
	end 
end

function _draw()
 cls()
 map()
 spr(sp,p.x,p.y,2,2,fx,fy)
 cx=p.x+4-63
 cy=p.y+4-63
 camera(cx,cy)
end
-->8
function onground(o)
  local check_x = o.x + 4
  local check_y = o.y + 9
  local tile = mget(check_x/8, check_y/8)
  local flag0 = fget(tile, 0) 

  return flag0       
end
